{
  "manifest_version": 3,
  "name": "Osu!direct Linker",
  "version": "0.1",
  "action": {
	"default_popup": "popup.html"
  },
  "description": "Adds an osu!direct link in beatmap search page",
  "permissions": [
    "activeTab",
	"storage"
  ],
  "content_scripts": [{
    "matches": ["https://osu.ppy.sh/beatmapsets",
				"https://osu.ppy.sh/beatmapsets?*"],
    "js": ["content.js"]
  }]
}
